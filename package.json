{
  "name": "quick-oi",
  "displayName": "Quick OI",
  "description": "Learning OI better.",
  "version": "1.1.1007",
  "author": {
    "email": "gugugupai233@gmail.com",
    "name": "pai233",
    "url": "https://github.com/pai233"
  },
  "publisher": "pai233",
  "engines": {
    "vscode": "^1.54.0"
  },
  "categories": [
    "Snippets",
    "Other"
  ],
  "keywords": [
		"luogu",
		"algorithm",
		"OI",
		"洛谷",
		"OJ",
		"OnlineJudge",
		"Vijos",
		"NOIp",
		"CSP",
		"ACM",
		"judge",
		"C++",
		"Pascal",
		"信息学",
		"奥赛",
    "模板",
    "Templates"
	],
  "activationEvents": [
    "onCommand:quick-oi.luogu.problem",
    "onCommand:quick-oi.vijos.problem",
    "onCommand:quick-oi.about",
    "onCommand:quick-oi.templates.import",
    "onCommand:quick-oi.templates.settings"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/pai233/quick-oi"
  },
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/pai233/quick-oi/issues",
    "email": "gugugupai233@gmail.com"
  },
  "icon": "icon.png",
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "quick-oi.luogu.problem",
        "title": "Luogu: Watch Problem"
      },
      {
        "command": "quick-oi.templates.import",
        "title": "Templates: Import"
      },
      {
        "command": "quick-oi.vijos.problem",
        "title": "Vijos: Watch Problem"
      }
      ,
      {
        "command": "quick-oi.about",
        "title": "Quick OI: About"
      },
      {
        "command": "quick-oi.templates.settings",
        "title": "Templates: Settings"
      }
    ],
    "keybindings": [
      {
        "command": "quick-oi.templates.import",
        "key": "Ctrl+Alt+T Ctrl+Alt+I"
      },
      {
        "command": "quick-oi.luogu.problem",
        "key": "Ctrl+Alt+L Ctrl+Alt+P"
      },
      {
        "command": "quick-oi.vijos.problem",
        "key": "Ctrl+Alt+V Ctrl+Alt+P"
      },
      {
        "command": "quick-oi.about",
        "key": "Ctrl+Alt+Q A"
      },
      {
        "command": "quick-oi.templates.settings",
        "key": "Ctrl+Alt+T Ctrl+Alt+S"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.54.0",
    "eslint": "^7.19.0",
    "glob": "^7.1.6",
    "mocha": "^8.2.1",
    "typescript": "^4.1.3",
    "vscode-test": "^1.5.0"
  },
  "dependencies": {
    "@luogu-dev/markdown-it-katex": "^0.0.1",
    "axios": "^0.21.1",
    "axios-cookiejar-support": "^1.0.1",
    "markdown-it": "^12.0.4",
    "tough-cookie": "^4.0.0"
  }
}
